SEQUENCER_SERVER_SRCS := $(shell find . -name '*.go')
SEQUENCER_SERVER_MAIN := main.go
SEQUENCER_SERVER_TARGET := sequencer_server

$(SEQUENCER_SERVER_TARGET): $(SEQUENCER_SERVER_SRCS)
	$(GO) build -o $(SEQUENCER_SERVER_TARGET) $(SEQUENCER_SERVER_MAIN)

clean:
	$(RM) -f $(SEQUENCER_SERVER_TARGET)

.PHONY: clean
